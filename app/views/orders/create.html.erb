<script src="https://js.stripe.com/v3/"></script>
<script type="text/javascript">
  var stripe_client = Stripe('<%= ENV['STRIPE_PUBLISHABLE_KEY'] %>');
  stripe_client.redirectToCheckout({sessionId: '<%= @session.id %>'}).then(function(result) {
